<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.alzheimerapp">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.AlzheimerApp">
        <activity android:name=".controllers.ListPatientActivity"></activity>
        <activity android:name=".controllers.CheckListActivity" />
        <activity android:name=".controllers.AddPatientActivity" />
        <activity android:name=".controllers.ForgetPasswordActivity" />
        <activity android:name=".controllers.RegisterActivity" />
        <activity android:name=".controllers.LoginActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".controllers.MainActivity" />
    </application>

</manifest>